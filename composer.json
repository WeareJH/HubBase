        "squizlabs/php_codesniffer": "~2.2",
        "satooshi/php-coveralls": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url" : "https://github.com/WeareJH/JhUser.git"

        },
        {
            "type": "vcs",
